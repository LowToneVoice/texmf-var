@article{TarantinoFidkowski1604,
  title     = {Discrete spin structures and commuting projector models for two-dimensional fermionic symmetry-protected topological phases},
  author    = {Tarantino, Nicolas and Fidkowski, Lukasz},
  journal   = {Phys. Rev. B},
