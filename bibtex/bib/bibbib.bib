@article{ElseThorngren1907TopoLSM,
  title     = {Topological theory of Lieb-Schultz-Mattis theorems in quantum spin systems},
  author    = {Else, Dominic V. and Thorngren, Ryan},
  journal   = {Phys. Rev. B},
  volume    = {101},
  issue     = {22},
