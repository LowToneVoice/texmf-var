@article{BravyiHastingsMichalakis1001,
  author   = {Bravyi, Sergey and Hastings, Matthew B. and Michalakis, Spyridon},
  title    = {Topological quantum order: Stability under local perturbations},
  journal  = {Journal of Mathematical Physics},
  volume   = {51},
  number   = {9},
  pages    = {093512},
  year     = {2010},
  month    = {09},
  abstract = {We study zero-temperature stability of topological phases of matter under weak time-independent perturbations. Our results apply to quantum spin Hamiltonians that can be written as a sum of geometrically local commuting projectors on a D-dimensional lattice with certain topological order conditions. Given such a Hamiltonian H0, we prove that there exists a constant threshold ϵ\\&gt;0 such that for any perturbation V representable as a sum of short-range bounded-norm interactions, the perturbed Hamiltonian H=H0+ϵV has well-defined spectral bands originating from low-lying eigenvalues of H0. These bands are separated from the rest of the spectra and from each other by a constant gap. The band originating from the smallest eigenvalue of H0 has exponentially small width (as a function of the lattice size). Our proof exploits a discrete version of Hamiltonian flow equations, the theory of relatively bounded operators, and the Lieb–Robinson bound.},
  issn     = {0022-2488},
  doi      = {10.1063/1.3490195},
  url      = {https://doi.org/10.1063/1.3490195},
  eprint   = {arXiv:1001.0344}
}

