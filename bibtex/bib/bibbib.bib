@article{HastingsWen0503,
  title     = {Quasiadiabatic continuation of quantum states: The stability of topological ground-state degeneracy and emergent gauge invariance},
  author    = {Hastings, M. B. and Wen, Xiao-Gang},
  journal   = {Phys. Rev. B},
  volume    = {72},
  issue     = {4},
